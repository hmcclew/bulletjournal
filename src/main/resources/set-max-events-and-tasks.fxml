             <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="182.0" prefWidth="130.0">
                  <children>
                     <Label fx:id="setMaxEvents" layoutX="80.0" layoutY="21.0" text="Set Maximum Events" />
                     <TextField fx:id="setMaxEventsContent" layoutX="70.0" layoutY="50.0" prefHeight="26.0" prefWidth="134.0" />
                     <Label fx:id="setMaxTasks" layoutX="80.0" layoutY="91.0" text="Set Maximum Tasks" />
                     <TextField fx:id="setMaxTasksContent" layoutX="70.0" layoutY="120.0" prefHeight="26.0" prefWidth="134.0" />
                  </children>
               </AnchorPane>
           </content>
         </TitledPane>
         <Button fx:id="finish" layoutX="190.0" layoutY="387.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="113.0" text="Finish" />
      </children></AnchorPane>
</Scene>

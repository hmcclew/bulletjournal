<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" width="500.0">
    <AnchorPane prefHeight="454.0" prefWidth="1514.0">
      <children>
         <Label layoutX="100.0" layoutY="38.0" text="Set Maximum Events and Tasks">
            <font>
               <Font size="25.0" />
            </font>
